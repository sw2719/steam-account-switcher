#: steamswitcher.py:168
msgid "Checking for updates..."
msgstr "업데이트 확인 중..."

#: steamswitcher.py:176 steamswitcher.py:447
msgid "Update"
msgstr "업데이트"

#: steamswitcher.py:183 steamswitcher.py:355 steamswitcher.py:793
#: steamswitcher.py:902 steamswitcher.py:998 steamswitcher.py:1143
#: steamswitcher.py:1287
msgid "Cancel"
msgstr "취소"

#: steamswitcher.py:185
msgid "Update now"
msgstr "지금 업데이트"

#: steamswitcher.py:190
#, python-format
msgid "New version %s is available."
msgstr "버전 %s이 사용 가능합니다."

#: steamswitcher.py:254 steamswitcher.py:347 steamswitcher.py:1337
#: steamswitcher.py:1358
msgid "Error"
msgstr "오류"

#: steamswitcher.py:255
msgid "Error occured while downloading update."
msgstr "업데이트를 다운로드하는 도중 오류가 발생했습니다."

#: steamswitcher.py:347
msgid "Couldn't perform automatic update."
msgstr "자동 업데이트를 수행할 수 없습니다."

#: steamswitcher.py:348
msgid "Update manually by extracting update.zip file."
msgstr "수동으로 진행하려면 update.zip 파일을 직접 압축해제하세요."

#: steamswitcher.py:355
msgid "Are you sure to cancel?"
msgstr "정말로 취소하시겠습니까?"

#: steamswitcher.py:381 steamswitcher.py:384
msgid "Info"
msgstr "정보"

#: steamswitcher.py:443
msgid "New update available"
msgstr "새 업데이트 사용 가능"

#: steamswitcher.py:456
msgid "Using the latest version"
msgstr "최신 버전 사용 중"

#: steamswitcher.py:462
msgid "Development version"
msgstr "개발 버전"

#: steamswitcher.py:468
msgid "Failed to check for updates"
msgstr "업데이트 확인 실패"

#: steamswitcher.py:519 steamswitcher.py:674
msgid "Registry Error"
msgstr "레지스트리 오류"

#: steamswitcher.py:520
msgid "Failed to read registry value."
msgstr "레지스트리 값을 읽을 수 없습니다."

#: steamswitcher.py:521
msgid "Make sure that Steam is installed."
msgstr "Steam이 설치되어 있는지 확인하세요."

#: steamswitcher.py:566
msgid "Auto-login Enabled"
msgstr "자동로그인 켜짐"

#: steamswitcher.py:568
msgid "Auto-login Disabled"
msgstr "자동로그인 꺼짐"

#: steamswitcher.py:600
msgid "Information"
msgstr "정보"

#: steamswitcher.py:601
msgid "With version 1.6, data format has been changed."
msgstr "버전 1.6 에서, 데이터 포맷이 변경되었습니다."

#: steamswitcher.py:602
msgid "Attempt to convert your account data has failed."
msgstr "자동으로 기존 데이터를 변환하는데 실패했습니다."

#: steamswitcher.py:603
msgid "Please add them manually or try again by restarting app."
msgstr "수동으로 다시 추가하거나 재시작하여 다시 시도하세요."

#: steamswitcher.py:674
msgid "Failed to change registry value."
msgstr "레지스트리 값을 바꿀 수 없습니다."

#: steamswitcher.py:690 steamswitcher.py:1403
msgid "About"
msgstr "정보"

#: steamswitcher.py:693
msgid "Made by sw2719 (Myeuaa)"
msgstr "Made by 꺔먕 (Myeuaa / sw2719)"

#: steamswitcher.py:699
msgid "Warning: The developer of this application is not responsible for"
msgstr "경고: 본 앱의 사용으로 인한 데이터 손실 및 기타 손해 등"

#: steamswitcher.py:700
msgid "data loss or any other damage from the use of this app."
msgstr "어떠한 사고나 문제에 대해서 제작자는 책임을 지지 않습니다."

#: steamswitcher.py:701
msgid "STEAM is a registered trademark of Valve Corporation."
msgstr "STEAM은 Valve Corporation의 등록 상표입니다."

#: steamswitcher.py:711
msgid "Close"
msgstr "닫기"

#: steamswitcher.py:730 steamswitcher.py:790
msgid "Add"
msgstr "추가"

#: steamswitcher.py:741
msgid "Enter accounts(s) to add."
msgstr "추가할 계정을 입력하세요."

#: steamswitcher.py:743
msgid "In case of adding multiple accounts,"
msgstr "다수의 계정을 입력할 경우에는,"

#: steamswitcher.py:744
msgid "seperate each account with '/' (slash)."
msgstr "'/'(슬래시)로 구분합니다."

#: steamswitcher.py:772
msgid "Duplicate Alert"
msgstr "중복 경고"

#: steamswitcher.py:773
#, python-format
msgid "Account %s already exists."
msgstr "계정 %s은 이미 추가되어 있습니다."

#: steamswitcher.py:809 steamswitcher.py:1330
msgid "Alert"
msgstr "경고"

#: steamswitcher.py:809
msgid "Could not load loginusers.vdf."
msgstr "파일 loginusers.vdf를 불러오지 못했습니다."

#: steamswitcher.py:810
msgid "This may be because Steam directory defined"
msgstr "레지스트리에 설정된 스팀 경로가 올바르지"

#: steamswitcher.py:811
msgid "in registry is invalid."
msgstr "않을 수 있습니다."

#: steamswitcher.py:812
msgid "Do you want to select Steam directory manually?"
msgstr "수동으로 스팀 경로를 설정하시겠습니까?"

#: steamswitcher.py:822
msgid "Warning"
msgstr "경고"

#: steamswitcher.py:823
msgid "Steam directory is invalid."
msgstr "스팀 경로가 올바르지 않습니다."

#: steamswitcher.py:824
msgid "Try again?"
msgstr "다시 시도할까요?"

#: steamswitcher.py:833 steamswitcher.py:906
msgid "Import"
msgstr "가져오기"

#: steamswitcher.py:843
msgid "Select accounts to import."
msgstr "가져올 계정들을 선택하세요."

#: steamswitcher.py:844
msgid "Added accounts don't show up."
msgstr "이미 추가된 계정은 나타나지 않습니다."

#: steamswitcher.py:918
msgid "No Accounts"
msgstr "계정 없음"

#: steamswitcher.py:919
msgid "There's no account to remove."
msgstr "삭제할 계정이 없습니다."

#: steamswitcher.py:922 steamswitcher.py:1002
msgid "Remove"
msgstr "삭제"

#: steamswitcher.py:929
msgid "Select accounts to remove."
msgstr "삭제할 계정을 선택하세요."

#: steamswitcher.py:1080
msgid "Drag or use buttons below"
msgstr "드래그하거나 아래의 버튼으로"

#: steamswitcher.py:1081
msgid "to change order."
msgstr "순서를 변경하세요."

#: steamswitcher.py:1132
msgid "Up"
msgstr "위"

#: steamswitcher.py:1136
msgid "Down"
msgstr "아래"

#: steamswitcher.py:1140 steamswitcher.py:1282
msgid "OK"
msgstr "확인"

#: steamswitcher.py:1147 steamswitcher.py:1292
msgid "Apply"
msgstr "적용"

#: steamswitcher.py:1168 steamswitcher.py:1401
msgid "Settings"
msgstr "설정"

#: steamswitcher.py:1179
msgid "Language"
msgstr "언어"

#: steamswitcher.py:1196
msgid "Restart app to apply language settings."
msgstr "언어 설정을 적용하려면 앱을 재시작하세요."

#: steamswitcher.py:1202
msgid "Show profile names"
msgstr "프로필 이름 표시"

#: steamswitcher.py:1206
msgid "Use bar - |"
msgstr "막대 사용 - |"

#: steamswitcher.py:1207
msgid "Use brackets - ( )"
msgstr "괄호 사용 - ( )"

#: steamswitcher.py:1208
msgid "Off"
msgstr "끄기"

#: steamswitcher.py:1222
msgid "Try to soft shutdown Steam client"
msgstr "스팀 클라이언트 안전 종료"

#: steamswitcher.py:1237
msgid "Exit app upon Steam restart"
msgstr "스팀 재시작시 앱 종료"

#: steamswitcher.py:1331
msgid "After soft shutdown attempt,"
msgstr "스팀 안전 종료를 시도했으나,"

#: steamswitcher.py:1332
msgid "Steam appears to be still running."
msgstr "Steam이 아직 실행 중인 것 같습니다."

#: steamswitcher.py:1333
msgid "Do you want to force shutdown Steam?"
msgstr "Steam을 강제 종료 하시겠습니까?"

#: steamswitcher.py:1338
msgid "Could not soft shutdown Steam."
msgstr "Steam을 안전 종료할 수 없었습니다."

#: steamswitcher.py:1339
msgid "App will now exit."
msgstr "앱이 종료됩니다."

#: steamswitcher.py:1359
msgid "Could not start Steam automatically"
msgstr "알 수 없는 이유로 Steam을"

#: steamswitcher.py:1360
msgid "for unknown reason."
msgstr "자동으로 실행 할 수 없습니다."

#: steamswitcher.py:1378
msgid "Account Switcher"
msgstr "Steam 계정 전환기"

#: steamswitcher.py:1392
msgid "Import accounts from Steam"
msgstr "Steam에서 계정 가져오기"

#: steamswitcher.py:1394
msgid "Add accounts"
msgstr "계정 추가"

#: steamswitcher.py:1396
msgid "Remove accounts"
msgstr "계정 삭제"

#: steamswitcher.py:1398
msgid "Change account order"
msgstr "계정 순서 변경"

#: steamswitcher.py:1406
msgid "Menu"
msgstr "메뉴"

#: steamswitcher.py:1413
msgid "Debug"
msgstr "디버그"

#: steamswitcher.py:1423
msgid "Toggle auto-login"
msgstr "자동로그인 토글"

#: steamswitcher.py:1428
msgid "Exit"
msgstr "종료"

#: steamswitcher.py:1434 steamswitcher.py:1557
msgid "Restart Steam & Exit"
msgstr "스팀 재시작후 종료"

#: steamswitcher.py:1436 steamswitcher.py:1559
msgid "Restart Steam"
msgstr "스팀 재시작"

#: steamswitcher.py:1447 steamswitcher.py:1469
msgid "No accounts added"
msgstr "추가된 계정 없음"

#: steamswitcher.py:1471
msgid "Current Auto-login user:"
msgstr "현재 자동로그인 계정:"

